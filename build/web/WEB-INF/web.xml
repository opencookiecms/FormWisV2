<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DeleteProject</servlet-name>
        <servlet-class>Servlet.DeleteProject</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditFish</servlet-name>
        <servlet-class>Servlet.EditFish</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteFish</servlet-name>
        <servlet-class>Servlet.DeleteFish</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteResearcher</servlet-name>
        <servlet-class>Servlet.DeleteResearcher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RViewbyID</servlet-name>
        <servlet-class>Servlet.RViewbyID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FViewbyID</servlet-name>
        <servlet-class>Servlet.FViewbyID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PViewbyID</servlet-name>
        <servlet-class>Servlet.PViewbyID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FviewAdd</servlet-name>
        <servlet-class>Servlet.FviewAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjectViewId</servlet-name>
        <servlet-class>Servlet.ProjectViewId</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FishVbyAR</servlet-name>
        <servlet-class>Servlet.FishVbyAR</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteProject</servlet-name>
        <url-pattern>/project_delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditFish</servlet-name>
        <url-pattern>/fish_edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteFish</servlet-name>
        <url-pattern>/fish_delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteResearcher</servlet-name>
        <url-pattern>/researcher_delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RViewbyID</servlet-name>
        <url-pattern>/researcher_view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FViewbyID</servlet-name>
        <url-pattern>/fish_view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PViewbyID</servlet-name>
        <url-pattern>/project_view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FviewAdd</servlet-name>
        <url-pattern>/AddFish</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProjectViewId</servlet-name>
        <url-pattern>/project_view_id</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FishVbyAR</servlet-name>
        <url-pattern>/fish_id</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
